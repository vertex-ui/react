{
  "name": "@vui/react",
  "version": "0.0.1-beta.1",
  "description": "A production-ready React + TypeScript UI library with enterprise-grade components",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./theme": {
      "types": "./dist/types/theme/index.d.ts",
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js"
    },
    "./package.json": "./package.json"
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/types/index.d.ts"
      ],
      "theme": [
        "dist/types/theme/index.d.ts"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "author": "Innostes Solutions Pvt Ltd <contact@innostes.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/vertex-ui/react"
  },
  "homepage": "https://innostes.com/",
  "bugs": {
    "url": "https://github.com/vertex-ui/react/issues"
  },
  "keywords": [
    "react",
    "react-component",
    "react-components",
    "ui",
    "ui-library",
    "component-library",
    "design-system",
    "typescript",
    "vtx-ui",
    "vertex-ui",
    "accessibility",
    "a11y",
    "themeable",
    "dark-mode",
    "enterprise"
  ],
  "scripts": {
    "build": "rollup -c",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": ">=17",
    "react-dom": ">=17"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^4.1.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@storybook/addon-a11y": "^10.1.10",
    "@storybook/addon-docs": "^10.1.10",
    "@storybook/addon-vitest": "^10.1.10",
    "@storybook/react-vite": "^10.1.10",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "eslint-plugin-storybook": "^10.1.10",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^30.2.0",
    "jest-axe": "^7.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "playwright": "^1.57.0",
    "rollup": "^2.79.2",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "storybook": "^10.1.10",
    "ts-jest": "^29.4.6",
    "vitest": "^4.0.16"
  },
  "dependencies": {
    "date-fns": "^4.1.0",
    "react-icons": "^5.5.0",
    "tslib": "^2.8.1"
  }
}
